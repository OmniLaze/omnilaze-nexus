{
    "family": "omnilaze-nexus",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
    "containerDefinitions": [{
        "name": "nexus",
        "image": "442729101249.dkr.ecr.ap-southeast-1.amazonaws.com/omnilaze-nexus:20250820-220836",
        "essential": true,
        "portMappings": [{
            "containerPort": 80,
            "protocol": "tcp"
        }],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/omnilaze-nexus",
                "awslogs-region": "ap-southeast-1",
                "awslogs-stream-prefix": "ecs"
            }
        },
        "healthCheck": {
            "command": ["CMD-SHELL", "curl -f http://localhost/health || exit 1"],
            "interval": 30,
            "timeout": 5,
            "retries": 3,
            "startPeriod": 60
        }
    }]
}
